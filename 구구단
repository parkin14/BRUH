import time
def times_table(n):
    a=int(n[0])
    b=int(n[1])
    h=0
    if a>b:
        h=a
        a=b
        b=h
    for j in range(a,b+1):
        print("== %d단 ==" %j)
        for i in range(1,10):
            print("%d × %d = %2d" % (j, i, j*i), end='\n')
print("구구단을 외자")
time.sleep(1)
print("구구단을 외자")
time.sleep(1)
n=input("몇단부터 몇단?  ").split()
times_table(n)
