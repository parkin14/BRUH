class TV:
    def __init__(self,ch,vol):
        self.channel=ch
        self.volume=vol
        self.power=True
    def on_off(self):
        if self.power:
            self.power=False
            print("Turn on")
        else:
            self.power=True
            print("Turn off")
    def info(self):
        print("Power :",self.power)
        print("Channel :",self.channel)
        print("Volume :",self.volume)
        
    def set_channel(self,ch):
        if self.power:
            self.channel=ch
        else:
            print("Power off")
    def set_volume(self,vol):
        if self.power:
            self.volume=vol
        else:
            print("Power off")
tv=TV(1,16)
tv.info()
tv.set_channel(5)
tv.set_channel(12)
tv.on_off()
tv.on_off()
